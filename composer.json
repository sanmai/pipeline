{
    "name": "sanmai/pipeline",
    "description": "Not your general purpose collection pipeline for PHP",
    "type": "library",
    "authors": [
        {
            "name": "Alexey Kopytko",
            "email": "alexey@kopytko.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": ">=6",
        "php-coveralls/php-coveralls": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.4",
        "codacy/coverage": "^1.4",
        "league/pipeline": "^0.3",
        "phan/phan": "^0.12.1",
        "infection/infection": "^0.8.1",
        "phpstan/phpstan": "^0.9.2"
    },
    "license": "Apache-2.0",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {"Pipeline\\": "src/Pipeline/"}
    },
    "autoload-dev": {
        "psr-4": {"Pipeline\\": "tests/Pipeline/"}
    },
    "require": {
        "php": ">=7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-future": "2.0.x-dev"
        }
    }
}
